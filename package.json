{
  "name": "banka-react",
  "version": "1.0.0",
  "description": "Frontend for banka project built with React",
  "main": "index.js",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|svg|cass|scss|less|css)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "collectCoverage": true,
    "setupFilesAfterEnv": [
      "<rootDir>/__tests__/setupTest.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/setupTest.js"
    ]
  },
  "scripts": {
    "dev": "webpack-dev-server --mode development --open",
    "build": "webpack --mode production",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "coveralls": "npm run test:coverage && cat ./coverage/lcov.info | coveralls",
    "test:watch": "jest --watch",
    "lint": "eslint src -c .eslintrc.js --ext js,jsx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tolulope-od/banka-react.git"
  },
  "keywords": [
    "finance",
    "react",
    "frontend"
  ],
  "author": "Tolulope Odueke",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tolulope-od/banka-react/issues"
  },
  "homepage": "https://github.com/tolulope-od/banka-react#readme",
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "coveralls": "^3.0.4",
    "css-loader": "^3.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.0.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "jest-config": "^24.8.0",
    "mini-css-extract-plugin": "^0.7.0",
    "prettier": "^1.18.2",
    "style-loader": "^0.23.1",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "redux": "^4.0.3",
    "redux-thunk": "^2.3.0"
  }
}
